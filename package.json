{
  "name": "school-management-system",
  "version": "1.0.0",
  "description": "Comprehensive School Management System with Core Functionality",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "node server/index.js",
    "setup": "node setup.js",
    "setup:db": "node setup.js",
    "seed": "node setup.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint server/",
    "lint:fix": "eslint server/ --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "clean": "rm -rf node_modules client/node_modules && npm run install-all",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "keywords": [
    "school",
    "management",
    "education",
    "lms"
  ],
  "author": "School Management Team",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "exceljs": "^4.3.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.4",
    "pdf-lib": "^1.17.1",
    "pg": "^8.16.3",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
